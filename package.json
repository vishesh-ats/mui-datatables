    "lodash.merge": "^4.6.2",
    "prop-types": "^15.7.2",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-to-print": "^3.2.0",
    "tss-react": "^4.9.20"
  },
  "side-effects": false,
  "nyc": {
    "lines": 50,
    "statements": 50,
    "functions": 50,
    "branches": 50,
    "check-coverage": true,
    "all": true,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "test/**/*.test.js"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
